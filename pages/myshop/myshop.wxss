page{
  width: 100%;
  height: 100%;
  background-color: #f7f4f4;
}

.container{
  padding: 100rpx 0;
  font-size: 38rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.top-btn{
  height: 80rpx;
  display: flex;
  position: relative;
  background-color: white;
  border-bottom: 1px solid #ccc;
}
#myMap{
  width: 100%;
  height: 400rpx;
}
.top-btn text{
  position: absolute;
  top: 50%;
  right: 10%;
  transform: translateY(-50%);
  font-size: 32rpx;
  line-height: 32rpx;
}
.labal{
  padding: 0 20rpx;
  display: flex;
  font-size: 32rpx;
  align-items: center;
  border-radius: 10rpx 10rpx 0 0; 
  background-color: white;
  margin-top: 20rpx; 
}
.labal text{
  margin-right: 10rpx;
}
.shop-detial{
  display: flex;
  height: 200rpx;
  margin-bottom: 20rpx;
  background-color: white;
  border-radius: 0 0 10rpx 10rpx;
}
.shop-img{
  flex: 4;
  padding: 20rpx;
}
.shop-mark{
  flex: 6;
  padding: 20rpx 40rpx;
  font-size: 32rpx;
  display: flex;
}
.shop-mark .right{
  flex: 1;
  
}
.mar10{
  margin-top: 20rpx;
}
.shop-mark .left{
  flex: 1;
}
.shop-detial image{
  height: 100%;
  width: 100%;
}
.status-btn {
  width: 100rpx;
  background-color: lightgreen;
  text-align: center;
  border-radius: 10rpx;
  color: cornsilk;
}
.status-btns {
  width: 100rpx;
  background-color: rgb(250, 124, 85);
  text-align: center;
  border-radius: 10rpx;
  color: cornsilk;
}
.address-text{
  padding: 20rpx;
  margin-bottom: 20rpx;
  background-color: white;
  border-radius: 0 0 10rpx 10rpx;
}

text{
  font-size: 32rpx;
}
.user-list{
  padding: 0 50rpx;
  display: flex;
  background-color: white;
  border-radius: 0 0 10rpx 10rpx;
}
.user-img{
  width: 70rpx;
  height: 70rpx;
  border-radius: 50%;
}
.use-name{
  flex: 1;
  display: flex;
  justify-content: center;
  align-items: center;
}
.btns{
  flex: 1;
  display: flex;
  justify-content: center;
  align-items: center;
}
.data-time {
  flex: 1;
  display: flex;
  justify-content: center;
  align-items: center;
}
.btns-fal{
  width: 100rpx;
  background-color: lightgreen;
  color: cornsilk;
  border-radius: 10rpx;
  text-align: center;
}
